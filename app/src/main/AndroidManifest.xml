<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.thales.firebase">

    <!-- APP TERÁ ACESSO A INTERNET -->
    <uses-permission android:name="android.permission.INTERNET" />

    <!-- APP TERÁ ACESSO PARA ARQUIVOS EXTERNOS -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.PrincipalActivity" />
        <activity android:name=".Activity.UploadFotoActivity" />
        <activity android:name=".Activity.CadastroUsuarioClienteActivity" />
        <activity android:name=".Activity.CadastroUsuarioProfissionalActivity" />
        <activity android:name=".Activity.UsuariosActivity"></activity>
    </application>

</manifest>